{
  "name": "shopping-list",
  "version": "1.0.0",
  "description": "Shopping List Desktop App",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . ShoppingList --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=duttaditya18 --version-string.FileDescription=duttaditya18 --version-string.ProductName=\"ShoppingList\"",
    "package-linux": "electron-packager . ShoppingList --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/icon.png --prune=true --out=release-builds"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/duttaditya18/learn-node-js.git"
  },
  "author": "duttaditya18",
  "license": "WTFPL",
  "bugs": {
    "url": "https://github.com/duttaditya18/learn-node-js/issues"
  },
  "homepage": "https://github.com/duttaditya18/learn-node-js#readme",
  "dependencies": {
    "electron": "^3.0.10"
  },
  "devDependencies": {
    "electron-packager": "^12.2.0"
  }
}
